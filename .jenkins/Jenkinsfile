@Library('c3i') _

def artifactoryID = "ArtifactoryConan"
def artifactoryCredentials = "c3i_on_artifactory"

def user
def password
def url = "https://conan.jfrog.io/conan"

withCredentials([usernamePassword(credentialsId: artifactoryCredentials,
                                  usernameVariable: 'ARTIUSER',
                                  passwordVariable: 'ARTIPASS')]) {
    user = env.ARTIUSER
    password = env.ARTIPASS
}

c3i.runBuild(this, url, user, password)

